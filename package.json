{
  "name": "budgets-api",
  "version": "1.0.0",
  "description": "REST API for budget tracker app",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/rugglcon/budgets-api"
  },
  "scripts": {
    "build": "tsc",
    "dev": "npm run build && node dist/server.js",
    "start": "npm run dev",
    "test": "jest",
    "generate-migration": "npm run build && node ./node_modules/typeorm/cli.js migration:generate -n",
    "run-migration": "npm run build && node ./node_modules/typeorm/cli.js migration:run",
    "revert-migration": "npm run build && node ./node_modules/typeorm/cli.js migration:revert"
  },
  "author": "Connor Ruggles",
  "license": "GPL-3.0",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.18.3",
    "concurrently": "^3.5.1",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-useragent": "^1.0.13",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.16.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.13",
    "typescript": "^3.5.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^2.0.0",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.16.1",
    "@types/express-useragent": "^0.2.22",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mysql": "^2.15.5",
    "@types/node": "^10.12.26",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/passport-local": "^1.0.33",
    "@types/winston": "^2.4.4",
    "express-jwt": "^5.3.1",
    "jest": "^24.1.0",
    "jest-cli": "^24.8.0",
    "nodemon": "^1.18.10",
    "ts-node": "^8.2.0"
  }
}
