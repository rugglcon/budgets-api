{
  "name": "budgets-api",
  "version": "1.0.0",
  "description": "REST API for budget tracker app",
  "main": "server.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec ./node_modules/.bin/ts-node -- ./src/server.ts",
    "start": "npm run dev",
    "test": "jest",
    "generate-migration": "typeorm migration:generate",
    "run-migration": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "revert-migration": "ts-node ./node_modules/typeorm/cli.js migration:revert"
  },
  "author": "Connor Ruggles",
  "license": "GPL-3.0",
  "dependencies": {
    "bcrypt": "^3.0.4",
    "body-parser": "^1.18.3",
    "concurrently": "^3.5.1",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.13",
    "typescript": "^2.9.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^2.0.0",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.12",
    "@types/mysql": "^2.15.5",
    "@types/node": "^10.12.24",
    "jest": "^23.1.0",
    "jest-cli": "^23.1.0",
    "mysql": "^2.16.0",
    "nodemon": "^1.18.3",
    "ts-node": "^7.0.0"
  }
}
